{
  "name": "svelte-flow",
  "version": "0.0.2",
  "description": "A highly customizable node-based graph editor for Svelte",
  "private": false,
  "homepage": "https://github.com/sidharth-anand/svelte-flow",
  "license": "MIT",
  "author": "Sidharth Anand <sidharthanand85@gmail.com> (https://github.com/sidharth-anand)",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "package": "svelte-kit package",
    "publish": "npm publish ./package",
    "start": "sirv public --no-clear -p 5000",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@sveltejs/kit": "^1.0.0-next.347",
    "@tsconfig/svelte": "^2.0.0",
    "@types/d3": "^7.1.0",
    "postcss": "^8.4.12",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.50.1",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "svelte2tsx": "^0.5.10",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@sidharth-anand/neodrag-svelte": "^1.1.6",
    "d3": "^7.4.4",
    "sirv-cli": "^2.0.0"
  }
}
